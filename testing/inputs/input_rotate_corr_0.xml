    <OperatorString>isotriplet_pion_pion A1gp_1 [P=(0,0,0) A1um SS_0] [P=(0,0,0) A1um TDO_1]</OperatorString>
    <OperatorString>isotriplet_pion_pion A1gp_1 [P=(0,0,0) A1um TDO_1] [P=(0,0,0) A1um TDU_2]</OperatorString>
     <HermitianMatrix/>
