       <MCEnsembleInfo>clover_s24_t128_ud840_s743</MCEnsembleInfo> 
    </MCObservables>
  <!--  <TweakEnsemble>
       <Rebin>2</Rebin>
       <Omissions> 0 1 78</Omissions> 
    </TweakEnsemble>  -->
